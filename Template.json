{
  "Name": "Brewmaster.VNetTest",
  "Description": "Vnet Test",
  "Version": "0.1.0",
  "Parameters": [
    {
      "Name": "VnetName",
      "Type": "String",
      "Description": "Name of Virtual Network",
      "Default": "vnet"
    },
    {
      "Name": "Region",
      "Type": "String",
      "TypeHint": "AzureRegionName",
      "Description": "Name of Azure region."
    },
    {
      "Name": "AffinityGroup",
      "Type": "String",
      "TypeHint": "AzureAffinityGroupName",
      "Description": "Name of Azure affinity group."
    },
  ],
  "AffinityGroup": {
    "Name": "{{AffinityGroup}}",
    "Region": "{{Region}}"
  },
  "StorageAccounts": [],
  "CloudServices": [],
  "DeploymentGroups": [],
  "Credentials": [],
  "ConfigSets": [],
  "Configurations": [],
  "Network": {
    "DnsServers": [
      {
        "Name": "test-dns1",
        "IPAddress": "10.0.0.4"
      },
      {
        "Name": "test-dns2",
        "IPAddress": "10.0.0.5"
      }
    ],
    "LocalSites": [],
    "VirtualSites": [
      {
        "Name": "{{VnetName}}",
        "AffinityGroup": "{{AffinityGroup}}",
        "AddressSpace": [
          "10.0.0.0/8"
        ],
        "Subnets": [
          {
            "Name": "test-subnet",
            "AddressPrefix": "10.0.0.0/24"
          }
        ],
        "DnsServers": [
          "test-dns1",
          "test-dns2"
        ],
        "VPNClientAddressSpaces": []
      }
    ]
  }
}